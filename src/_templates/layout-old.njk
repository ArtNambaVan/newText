{% if not(title) %}
    {% set title = "Default title" %}
{% endif %}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{{ title }}</title>
    <link rel="stylesheet" href="css/libs/clay.css"> 

    <link rel="stylesheet" href="css/libs/main.css">
    {% block styles %} {% endblock styles %}
    
    <link rel="stylesheet" href="css/libs/treeJS.css">
    <link rel="stylesheet" href="css/custom.css">
    <script src="js/libs.js"></script>

    {% block scriptsHead %}{% endblock scriptsHead%}
    

</head>

<body>

    <div class="cims" id="wrapper">
        {% include "old-cims/header.njk" %}

        <main>
            <div class="center1080">
                {% block content %}

                {% endblock %}
            </div>
        </main>

        {% include "old-cims/footer.njk" %}
        
    </div>


    <!-- SCRIPTS -->
    {% block scripts %}
    
    {% endblock %}
    <script src="js/main.js"></script>
</body>
</html>