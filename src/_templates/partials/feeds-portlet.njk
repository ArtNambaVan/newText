
                <div class="container">
                    <h2>Feed Category</h2>
                    <div class="inline-form">
                        <span class="inline-form__item">New Feed Category</span>
                        <div class="input-small">
                            <input type="text" class="form-control" v-model="newFeedCat">
                        </div>
                        <button class="cims-btn cims-btn--primary" v-on:click="saveNewCat">Save</button>
                        <button class="cims-btn cims-btn--default">Cancel</button>
                    </div>

                    <el-table
                    :data="tableData"
                    style="width: 100%"
                    max-height="650"
                    >
                        <el-table-column
                            prop="name"
                            label="Name"
                            
                            >
                        </el-table-column>
                        <el-table-column
                            prop="id"
                            label="id"
                            
                            >
                        </el-table-column>
                        <el-table-column
                            prop="newMessage"
                            label="New Message"
                            
                            >
                        </el-table-column>
                        <el-table-column
                            prop=""
                            label="Options"
                            align="right"
                            >

                            <template slot-scope="scope">
                                <span class="interact" v-on:click="editElement(scope)">
                                    <i class="fas fa-edit"></i>
                                </span>
                                <span class="interact">
                                    <i class="fas fa-trash-alt"></i>
                                </span>
                            </template>
                        </el-table-column>
                    </el-table>
                    
                </div>
                <el-dialog title="Edit Feed Name" :visible.sync="dlgEdit" custom-class="">
                    <div class="dialog-body">
                        <h2>Edit Feed Name</h2>
                        <div class="inline-form">
                            <span class="inline-form__item">New Feed name</span>
                            <div class="input-small">
                                <input type="text" class="form-control" v-model="currentCat">
                            </div>
                        </div>
                    </div>
                    <div slot="footer" class="dialog-footer">
                        <el-button size="small" >Cancel</el-button>
                        <el-button size="small" type="primary" >Confirm</el-button>
                    </div>
                </el-dialog>



