                                    >
                                    <span slot-scope="{ node, data }">
                                        <el-checkbox class="disabled-checkbox" v-model="node.checked"></el-checkbox>
                                        <a href="javascript:void(0)" :class="'jstree-anchor' + (data.ratingCategory === true?' jstree-rating':'') + (data.assigned === false?'':' tree-category--disabled')" >
                                            {% raw %} {{ data.name }} {% endraw %}
                                        </a>
                                    </span>
                                </el-tree>
                            </div>
                            <button class="cims-btn cims-btn--primary mt-3">Add root</button>
            
                            <h2 class="mt-4">Selected nodes:</h2>
                            <ol class="bullet">
                                <li class="assignment-rule__item">
                                    <span class="category__name">CIMS-Restructure</span>
                                    <span class="interact" ><i class="fas fa-times-circle"></i></span>
                                </li>
                                <li class="assignment-rule__item">
